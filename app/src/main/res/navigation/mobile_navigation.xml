<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_attendance">

    <fragment
        android:id="@+id/navigation_attendance"
        android:name="com.example.collegehelper.ui.attendance.AttendanceFragment"
        android:label="@string/title_attendance"
        tools:layout="@layout/fragment_attendance" />

    <fragment
        android:id="@+id/navigation_assignment"
        android:name="com.example.collegehelper.ui.assignment.AssignmentFragment"
        android:label="@string/title_assignment"
        tools:layout="@layout/fragment_assignment" />

    <fragment
        android:id="@+id/navigation_notes"
        android:name="com.example.collegehelper.ui.notes.NotesFragment"
        android:label="@string/title_notes"
        tools:layout="@layout/fragment_notes" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.collegehelper.ui.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_setting" />
</navigation>